{
  "name": "mintter",
  "version": "0.0.1",
  "description": "Mintter monorepo",
  "repository": "https://github.com/mintterteam/mintter",
  "author": "Mintter Team",
  "private": true,
  "scripts": {
    "prebuild": "yarn validate",
    "build": "yarn workspaces foreach run build",
    "lint": "yarn workspaces foreach lint",
    "format": "yarn workspaces foreach format",
    "validate": "yarn workspaces foreach validate",
    "ui": "yarn workspace @mintter/ui",
    "app": "yarn workspace @mintter/app",
    "client": "yarn workspace @mintter/client",
    "mttast": "yarn workspace @mintter/mttast",
    "builder": "yarn workspace @mintter/mttast-builder",
    "hastmtt": "yarn workspace @mintter/hastmtt"
  },
  "workspaces": [
    "frontend/*"
  ],
  "devDependencies": {
    "covector": "^0.7.2",
    "eslint": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "ts-proto": "^1.83.0",
    "typescript": "4.4.4"
  },
  "packageManager": "yarn@3.1.0"
}
