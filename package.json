{
  "name": "mintter",
  "version": "0.0.1",
  "description": "Mintter monorepo",
  "repository": "https://github.com/mintterteam/mintter",
  "author": "Mintter Team",
  "private": true,
  "scripts": {
    "build": "yarn app build",
    "lint": "yarn app lint",
    "format": "yarn app format",
    "test": "yarn app test",
    "test:unit": "yarn app test:unit",
    "app": "yarn workspace @mintter/app",
    "codegen": "graphql-codegen",
    "validate": "yarn app validate"
  },
  "workspaces": [
    "frontend/*"
  ],
  "devDependencies": {
    "@types/wouter": "2.2.0",
    "app": "0.1.0",
    "covector": "0.7.3",
    "eslint": "8.20.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rollup": "2.77.0",
    "tiny-glob": "0.2.9",
    "ts-proto": "1.118.0",
    "typescript": "4.7.4"
  },
  "packageManager": "yarn@3.2.3",
  "dependencies": {
    "@graphql-codegen/cli": "2.9.0",
    "@graphql-codegen/typescript": "2.7.2",
    "ni": "0.0.2"
  }
}
