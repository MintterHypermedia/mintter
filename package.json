{
  "name": "mintter",
  "version": "0.0.1",
  "description": "Mintter monorepo",
  "repository": "https://github.com/mintterteam/mintter",
  "author": "Mintter Team",
  "private": true,
  "scripts": {
    "build": "yarn mttast build && yarn app build",
    "lint": "yarn mttast lint && yarn app lint",
    "format": "yarn mttast format && yarn app format",
    "test": "yarn app test && yarn mttast test",
    "test:unit": "yarn mttast test:unit && yarn app test:unit",
    "app": "yarn workspace @mintter/app",
    "mttast": "yarn workspace @mintter/mttast",
    "codegen": "graphql-codegen",
    "validate": "yarn mttast validate && yarn app validate"
  },
  "workspaces": [
    "frontend/*"
  ],
  "devDependencies": {
    "@types/wouter": "2.2.0",
    "app": "0.1.0",
    "covector": "0.7.3",
    "eslint": "8.10.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "rollup": "2.74.1",
    "tiny-glob": "0.2.9",
    "ts-proto": "1.112.2",
    "typescript": "4.7.2"
  },
  "packageManager": "yarn@3.2.1",
  "dependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.4.11",
    "ni": "0.0.2"
  }
}
