{
  "name": "mintter",
  "version": "0.0.1",
  "description": "Mintter monorepo",
  "repository": "https://github.com/mintterteam/mintter",
  "author": "Mintter Team",
  "private": true,
  "scripts": {
    "build": "yarn workspaces foreach -ptR --from '{@mintter/mttast,@mintter/app}' run build",
    "lint": "yarn workspaces foreach -ptR --from '{@mintter/mttast,@mintter/app}' run lint",
    "format": "yarn workspaces foreach -ptR --from '{@mintter/mttast,@mintter/app}' run format",
    "test": "yarn workspaces foreach -ptR --from '{@mintter/mttast,@mintter/app}' run test",
    "app": "yarn workspace @mintter/app",
    "mttast": "yarn workspace @mintter/mttast",
    "codegen": "graphql-codegen"
  },
  "workspaces": [
    "frontend/*"
  ],
  "devDependencies": {
    "@types/wouter": "2.2.0",
    "app": "0.1.0",
    "covector": "0.7.3",
    "eslint": "8.10.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "rollup": "2.69.0",
    "ts-proto": "1.106.2",
    "typescript": "4.4.4"
  },
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.4.5",
    "ni": "0.0.2"
  }
}
