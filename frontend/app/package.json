{
  "name": "@mintter/app",
  "version": "0.3.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "dev2": "VITE_MINTTER_API_URL=http://localhost:56001 && vite --port 4000",
    "build": "vite build",
    "serve": "vite preview",
    "test": "jest --maxWorkers=50%",
    "test:w": "jest --watch --maxWorkers=25%",
    "test:c": "jest --coverage",
    "test:ci": "jest --runInBand",
    "precommit": "lint-staged",
    "lint": "eslint .",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(ts|tsx|json)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "validate": "yarn check-types && yarn check-format && yarn lint && yarn test"
  },
  "dependencies": {
    "@mintter/client": "^0.2.1",
    "@mintter/ui": "^0.2.1",
    "@radix-ui/react-accessible-icon": "0.0.15",
    "@radix-ui/react-alert-dialog": "0.0.19",
    "@radix-ui/react-context-menu": "0.0.23",
    "@radix-ui/react-dropdown-menu": "^0.0.22",
    "@radix-ui/react-popover": "0.0.19",
    "@radix-ui/react-portal": "0.0.14",
    "@radix-ui/react-separator": "0.0.14",
    "@radix-ui/react-slot": "0.0.12",
    "@radix-ui/react-tooltip": "0.0.20",
    "@udecode/slate-plugins": "1.0.0-next.37",
    "@xstate/react": "^1.3.4",
    "autosize": "^5.0.0",
    "date-fns": "^2.22.1",
    "framer-motion": "^4.1.17",
    "is-plain-object": "^5.0.0",
    "react": "^17.0.0",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.0",
    "react-error-boundary": "^3.1.3",
    "react-hook-form": "6",
    "react-hot-toast": "^2.0.0",
    "react-lazily": "^0.9.0",
    "react-merge-refs": "^1.1.0",
    "react-query": "^3.16.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-spinners": "^0.11.0",
    "reakit": "^1.3.8",
    "show-keys": "^1.1.1",
    "slate": "https://gitpkg.now.sh/mintterteam/slate/packages/slate?v0.61.4-mtt",
    "slate-history": "https://gitpkg.now.sh/mintterteam/slate/packages/slate-history?v0.61.4-mtt",
    "slate-hyperscript": "https://gitpkg.now.sh/mintterteam/slate/packages/slate-hyperscript?v0.61.4-mtt",
    "slate-react": "https://gitpkg.now.sh/mintterteam/slate/packages/slate-react?v0.61.4-mtt",
    "slugify": "^1.5.3",
    "use-dark-mode": "^2.3.1",
    "xstate": "^4.20.0"
  },
  "devDependencies": {
    "@mintter/mttast-builder": "^0.1.0",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.0",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.15",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@typescript-eslint/typescript-estree": "^4.26.1",
    "@udecode/slate-plugins-test-utils": "^1.0.0-alpha.18",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint": "^7.28.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.0.4",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.0.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "ts-jest": "^27.0.2",
    "typescript": "^4.3.2",
    "vite": "^2.3.5",
    "vite-react-jsx": "^1.1.1",
    "vite-tsconfig-paths": "^3.3.13"
  },
  "lint-staged": {
    "src/**/*.+(ts|tsx)": [
      "yarn validate"
    ]
  }
}
