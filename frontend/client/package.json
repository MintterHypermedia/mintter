{
  "name": "@mintter/client",
  "version": "0.2.1",
  "main": "index.js",
  "license": "MIT",
  "exports": {
    "./hooks": "./hooks/index.ts",
    ".": "./src/index.ts",
    "./mocks": "./mocks/index.ts"
  },
  "scripts": {
    "test": "true",
    "test:w": "true",
    "test:coverage": "true",
    "precommit": "lint-staged",
    "build": "true",
    "lint": "eslint .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(ts|tsx|json)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "validate": "yarn check-format && yarn lint && yarn test"
  },
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.14.0",
    "@types/faker": "^5.5.6",
    "@types/google-protobuf": "^3.15.2",
    "@types/unist": "^2.0.6",
    "faker": "^5.5.3",
    "google-protobuf": "^3.17.2",
    "long": "^4.0.0",
    "protobufjs": "^6.11.2",
    "react-query": "^3.16.1",
    "unist-builder": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "ts-jest": "^27.0.2",
    "typescript": "^4.3.2"
  },
  "lint-staged": {
    "src/**/*.+(ts|tsx)": [
      "yarn validate"
    ]
  }
}
