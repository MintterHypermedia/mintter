{
  "name": "@mintter/gateway",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others --kill-others-on-fail -n POSTCSS,NEXT -c bgGreen,bgMagenta 'pnpm dev:styles' 'pnpm dev:front'",
    "dev:styles": "nodemon css-props-generator.js",
    "dev:front": "next dev",
    "build": "pnpm build:styles && pnpm build:front",
    "build:styles": "node css-props-generator.js",
    "build:front": "next build",
    "lint": "next lint",
    "format": "prettier --write .",
    "test": "echo 'NO TESTS YET';",
    "validate": "pnpm lint && pnpm test",
    "start": "next start"
  },
  "dependencies": {
    "@mintter/shared": "workspace:*",
    "@mdx-js/loader": "2.1.5",
    "@mdx-js/react": "2.1.5",
    "@next/mdx": "13.0.6",
    "@tanstack/react-query": "4.19.1",
    "@tanstack/react-query-devtools": "4.19.1",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@vercel/og": "^0.0.21",
    "@xstate/react": "3.0.1",
    "browser-headers": "0.4.1",
    "clsx": "^1.2.1",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "is-plain-object": "5.0.0",
    "js-video-url-parser": "0.5.1",
    "long": "5.2.1",
    "nanoid": "4.0.0",
    "next": "13.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "shiki-es": "^0.1.2",
    "slate": "^0.87.0",
    "slate-react": "^0.87.0",
    "typescript": "4.9.4",
    "unist-util-visit": "4.1.1",
    "xstate": "4.35.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "0.5.8",
    "@types/unist": "2.0.6",
    "autoprefixer": "10.4.13",
    "concurrently": "7.6.0",
    "cssnano": "5.1.14",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-storybook": "0.6.8",
    "eslint-plugin-tailwindcss": "^3.7.1",
    "nodemon": "2.0.20",
    "postcss": "8.4.20",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-nesting": "10.2.0",
    "postcss-partial-import": "4.1.0",
    "postcss-preset-env": "7.8.3",
    "prettier": "2.8.1",
    "tailwindcss": "3.2.4",
    "vercel": "^28.9.0",
    "vitest": "0.25.5",
    "webpack": ">=5.0.0 <6.0.0"
  },
  "engines": {
    "node": ">=14"
  }
}
