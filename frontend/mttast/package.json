  "devDependencies": {
    "@mintter/mttast": "^0.1.0",
    "@types/node": "16.11.36",
    "@types/unist": "2.0.6",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "is-hidden": "2.0.0",
    "ni": "0.0.2",
    "prettier": "2.6.2",
    "rehype-preset-minify": "6.0.0",
    "shiki": "0.9.15",
    "typescript": "4.4.4",
    "unified": "10.1.2",
    "vitest": "0.3.6"
