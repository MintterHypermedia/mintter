{
  "name": "@mintter/mttast",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "build": "tsc",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check \"**/*.{css,md,js,jsx,json,ts,tsx}\"",
    "lint": "yarn lint:prettier && yarn lint:eslint",
    "format": "prettier --write .",
    "test": "yarn test:unit",
    "test:unit": "vitest --run",
    "test:unit:watch": "test --watch",
    "validate": "yarn format && yarn lint && yarn test"
  },
  "devDependencies": {
    "@types/node": "16.11.42",
    "@types/unist": "2.0.6",
    "@typescript-eslint/eslint-plugin": "5.30.3",
    "@typescript-eslint/parser": "5.30.3",
    "@typescript-eslint/typescript-estree": "5.30.3",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "is-hidden": "2.0.0",
    "prettier": "2.7.1",
    "shiki": "0.10.1",
    "typescript": "4.7.4",
    "unified": "10.1.2",
    "vitest": "0.16.0"
  },
  "dependencies": {
    "deepmerge": "4.2.2",
    "hast-util-has-property": "2.0.0",
    "hast-util-is-element": "2.1.2",
    "hast-util-to-text": "3.1.1",
    "hastscript": "7.0.2",
    "is-plain-object": "5.0.0",
    "nanoid": "4.0.0",
    "rehype-minify-whitespace": "5.0.1",
    "rehype-parse": "8.0.4",
    "rehype-sanitize": "5.0.1",
    "trim-trailing-lines": "2.0.0",
    "unist-builder": "3.0.0",
    "unist-util-remove-position": "4.0.1",
    "unist-util-visit": "4.1.0"
  }
}
